{
  "name": "service-example",
  "vendor": "acctglobal",
  "version": "0.3.0",
  "title": "Service Example",
  "description": "Reference app for VTEX IO Services",
  "mustUpdateAt": "2018-01-04",
  "categories": [],
  "dependencies": {
    "vtex.catalog-api-proxy": "0.x",
    "vtex.catalog-graphql": "1.x"
  },
  "builders": {
    "node": "7.x",
    "docs": "0.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "httpstat.us",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "rickandmortyapi.com",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/segments/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "acctglobal.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "vtex.catalog-graphql:resolve-graphql"
    },
    {
      "name": "ViewProduct"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "POWER_USER_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "vbase-read-only"
    },
    {
      "name": "vbase-read-write"
    }
  ],
  "settingsSchema": {
    "title": "backend Example Settings",
    "description": "Settings for Service Example",
    "type": "object",
    "properties": {
      "appKeyExample": {
        "type": "string",
        "description": "Um exemplo de chave appkey",
        "default": "vtexappkey-acctglobal-OUNTJH"
      },
      "appTokenExample": {
        "type": "string",
        "description": "Um exemplo de chave token",
        "default": "TWXAQTZVQLUMZRBLIHZKMSBRRVMAMXLNDHKBOLVDVKKXHQRGDUWMVEXYPTODPPBLBXQPKQEESSYMGXLXADMDAHGOTTIKYUBHAVQYKZJWHXCPVEJNLTDMFQIPQQYGIJEY"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
